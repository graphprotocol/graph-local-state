{
  "name": "@graphprotocol/graph-local-sql-schema",
  "version": "0.0.0",
  "description": "Generate SQL schema from GraphQL schema",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "prepack": "bob prepack",
    "check": "tsc --pretty --noEmit"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.5",
    "@graphql-tools/load": "^7.5.3",
    "graphql": "^16.3.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.180",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
